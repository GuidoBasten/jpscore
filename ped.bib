@Article{Koh2011,
  Title                    = {Modeling and simulation of pedestrian behaviors in crowded places},
  Author                   = {Koh, Wee Lit and Zhou, Koh Suiping},
  Journal                  = {ACM Transactions on Modeling and Computer Simulation},
