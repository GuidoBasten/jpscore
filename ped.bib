@Article{Lippert1998,
  Title                    = {Hyper-systolic parallel computing},
  Author                   = {Lippert, Thomas and Seyfried, Armin and Bode, Achim and Schilling, KLaus},
  Journal                  = {IEEE Transactions on Parallel and Distributed Systems},
  Year                     = {1998},
  Pages                    = {97--108},
  Volume                   = {9},

  Abstract                 = {We introduce a new class of parallel algorithms for the exact computation of systems with pairwise mutual interactions of n elements, so called n(hoch2)-problems. Hitherto, practical conventional parallelization strategies could achieve a complexity of O(np) with respect to the inter-processor communication, p being the number of processors. Our new approach can reduce the interprocessor communication complexity to a number O(np(hoch1/2)) . In the framework of Additive Number Theory, the determination of the optimal communication pattern can be formulated as h-range minimization problem that can be solved numerically. Based on a complexity model, the scaling behavior of the new algorithm is numerically tested on the connection machine CM5. As a real life example, we have implemented a fast code for globular cluster n-body simulations, a generic n(hoch2)-problem, on the CRAY T3D, with striking success. Our parallel method promises to be useful in various scientific and engineering fields like polymer chain computations, protein folding, signal processing, and, in particular, for parallel level-3 BLAS.},
  Doi                      = {10.1109/71.663861},
  File                     = {Lippert1998.pdf:pdf/Lippert1998.pdf:PDF},
  Keywords                 = {Systolic algorithm, hyper-systolic algorithm, n-body computation, n(hoch2)-loop computation, parallel computer, connection machine CM5 and Cray T3D, novel complexity class},
