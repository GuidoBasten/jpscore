}

@InProceedings{Forell2010,
  Title                    = {Assessment of occupant safety in places of assembly},
  Author                   = {Forell, B. and Hosser, D.},
  Year                     = {2010},
  Organization             = {vfdb International Fire Protection Symposium 2010},

  Abstract                 = {Occupant safety in places of entertainment is assessed by a model of hazard and vulnerability. Fire hazards of different strength are simulated numerically in parameter studies with different fire growth coefficients. Hereby empirical correlations of the available safe escape time with the fire growth coefficient, the tenability criteria, and the size as well as the fire protection infrastructure of an assembly room are found. The evacuation process consists of the pre-movement time, which depends on the fire growth coefficient, and the movement time, which is calculated by capacity analysis. Limit states are expressed by critical fire growth coefficients for different tenability criteria. The probability of occurrence of serious incidents in assembly rooms as well as the individual risk of being seriously affected by a fire is determined regarding the German building code. The quantitative results are promising in comparison with literature data. The methodology can also be used to illustrate how the risk is affected by changes in the fire safety design. An indication on how occupant safety behaves in other countries is given by a comparison of exit-flow times according to nine different building codes.},
