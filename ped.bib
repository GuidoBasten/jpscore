@Article{LiuX2009,
  Title                    = {Extraction and quantitative analysis of microscopic evacuation characteristics based on digital image processing},
  Author                   = {Liu, Xuan and Song, Weiguo and Zhang, Jun},
  Journal                  = {Physica A: Statistical Mechanics and its Applications},
  Year                     = {2009},

  Month                    = {jul},
  Number                   = {13},
  Pages                    = {2717-2726},
  Volume                   = {388},

  Abstract                 = {In this paper, experiments of single-file pedestrian movement are conducted and the movement parameters of pedestrians are extracted with digital image processing method based on mean-shift algorithm. The microscopic characteristics of pedestrian dynamic including velocity, density, lateral oscillation etc, as well as their interrelations are obtained and analyzed. Firstly, we studied the lateral oscillation phenomena of pedestrian movement. The result indicates that the trajectory of pedestrians presents a wavy form and the amplitude of the oscillation keeps about 5.5 cm when the pedestrians move with free walking velocity, which is the velocity when there is no obvious interaction between sequential pedestrians; but when the movement velocity decreases to 0.27 m/s, the amplitude of oscillation increases to 13 cm. With the increasing density, the velocity decreases and the amplitude of oscillation presents a linear increase trend. The increasing oscillation amplitude widens occupation area of a pedestrian with low velocity, so as to make the moving efficiency even worse. Secondly, we study the frequency of the oscillation, the result indicates that the frequency maintains 2 Hz when pedestrians move with a free walking velocity, but presents a similar linear decrease trend when the velocity turns to a lower value. The decrease of oscillation frequency is also a negative feedback to the moving efficiency. Thirdly, it is found that with the increase of crowd density, the time interval between two sequential pedestrians increases, though the space gap between them decreases. The quantitative relation between time interval and crowd density is obtained. The study in this paper provides fundamental data and basic method for understanding pedestrian dynamics, developing and validating evacuation models. The results are also expected to be useful for the evacuation design.},
  Comment                  = {pedestrians, fundamental diagram, experiment, data collection, trajectories, digital image processing},
  Doi                      = {10.1016/j.physa.2009.03.017},
  File                     = {:pdf\\LiuX2009.pdf:PDF},
  Keywords                 = {Evacuation; Pedestrian flow; Digital image processing; Mean-shift algorithm; Experiment},
  Owner                    = {portz},
  Timestamp                = {2009.03.27}
}

