@Article{Kretz2008a,
  Title                    = {Upstairs walking speed distributions on a long stairway},
  Author                   = {Kretz, T. and Gr\"unebohm, Anna and Kessel, Andreas and Kl\"upfel, H. and Meyer-K\"onig, T. and Schreckenberg, M.},
