@Article{LinC2011,
  Title                    = {Bidirectional transport and pulsing states in a multi-lane ASEP model},
  Author                   = {Lin, Congping and Steinberg, Gero and Ashwin, Peter},
  Journal                  = {Journal of Statistical Mechanics: Theory and Experiment},
  Year                     = {2011},
  Number                   = {09},
  Pages                    = {P09027},
  Volume                   = {2011},

  __markedentry            = {[Matthias:]},
  Abstract                 = {In this paper, we introduce an ASEP-like transport model for bidirectional motion of particles on a multi-lane lattice. The model is motivated by in vivo experiments on organelle motility along a microtubule (MT), consisting of thirteen protofilaments, where particles are propelled by molecular motors (dynein and kinesin). In the model, organelles (particles) can switch directions of motion due to 'tug-of-war' events between counteracting motors. Collisions of particles on the same lane can be cleared by switching to adjacent protofilaments (lane changes). We analyse transport properties of the model with no-flux boundary conditions at one end of a MT ('plus end' or tip). We show that the ability to make lane changes can affect the transport efficiency and the particle-direction change rate obtained from experiments is close to optimal for achieving efficient motor and organelle transport in a living cell. In particular, we find a nonlinear scaling of the mean tip size (the number of particles accumulated at the tip) with injection rate and an associated phase transition leading to pulsing states characterized by periodic filling and emptying of the system.},
  Comment                  = {molecular motors, modelling, celullar automata},
  File                     = {:pdf\\LinC2011.pdf:PDF},
  Owner                    = {mmueller},
  Timestamp                = {2011.12.19},
  Url                      = {http://stacks.iop.org/1742-5468/2011/i=09/a=P09027}
}

@Article{LinP2007,
