@Proceedings{Pauls2008,
  Title                    = {Demographic changes leading to deterioration of pedestrian capabilities affecting falls safety and crowd movement performance including facility evacuation},
  Year                     = {2008},
  Note                     = {Transportation Research Board Annual Meeting},
  Organization             = {Transportation Research Board Annual Meeting},

  Abstract                 = {Two major demographic changes are affecting many countries. First identified was the "graying" of the population, more correctly, the growing proportion of the population having an advanced age, say 65 years. Second is the relatively recently recognized set of changes sometimes characterized (incompletely) as the "obesity epidemic" which is affecting all ages. The second demographic change has primary characteristics such as increased body mass, increased size (especially girth), and decreased physical fitness. Secondary characteristics are various health effects, some of which were formerly more associated with aging; these include hypertension, type 2 diabetes, joint deterioration, and some mental performance changes as well as movement performance changes including reduced walking speed and endurance. These changes could, in turn, affect walking-related fall and injury propensity plus the ability to cope with the physical demands of sustained crowd movement, for example, associated with evacuation of major facilities including high-occupancy buildings such as sports venues and high-rise structures. The time, as well as necessary organizational preparedness, required to accomplish such relatively demanding crowd movement will increase with these demographic changes. In terms of needed design improvements, consideration must now be given to increasing sidewalk and passageway width -- especially stairways -- to accommodate enlarged lateral dimensions of people as well as increased lateral sway behavior exacerbated with relatively slow movement speeds. Consideration must also be given to improving the usability of stairs, including lower step height, and increased provision of rest areas which, for high-rise building evacuations, might dictate increased stair landing sizes.},
  Author                   = {Pauls, Jake},
  File                     = {Pauls2008.pdf:pdf/Pauls2008.pdf:PDF},
  Owner                    = {portz},
  Timestamp                = {2008.06.13}
}

@InProceedings{Pauls2008a,
  Title                    = {Performance of Means of Egress Conducting the Research Needed to Establish Realistic Expectations},
  Author                   = {Pauls, Jake},
  Booktitle                = {For Proceedings of 7th International Conference on Performance-Based Codes and Fire Safety Design Methods, Auckland},
  Year                     = {2008},
  Organization             = {For Proceedings of 7th International Conference on Performance-Based Codes and Fire Safety Design Methods, Auckland},

  Abstract                 = {Large portions of the current technology of means of egress performance are based on studies performed decades ago in a relatively small number of contexts. The current validity of such studies, in all contexts, is now being critically questioned by some researchers -- namely Pauls and Fruin -- who were responsible for much of the original work. Chief among reasons for this is the apparent, rapidly reducing movement capability of people in most areas of the world due to demographic changes, especially reduced fitness levels brought on by increasingly sedentary lifestyles, increased body size and increased body weight. The term, "obesity epidemic," is a commonly applied label for the demographic changes, however, this tends to understate or misrepresent the problem as being merely one of excess weight. It is not. Neither is the problem solely one of reduced building occupant capabilities due, in part, to reduced individual and societal motivation plus physiological condition. Paralleling the deterioration of occupant performance capabilities is an apparent failure within safety professions -- more specifically fire protection engineering -- that, in this author's opinion, are not responding sufficiently in either a reactive or proactive fashion. Fire protection engineers and related safety plus design professionals, for example, are neither utilizing existing research properly nor facilitating future research needed to establish realistic expectations of egress performance for buildings.},
  File                     = {Pauls2008a.pdf:pdf/Pauls2008a.pdf:PDF},
  Owner                    = {portz},
  Timestamp                = {2008.06.13}
}

@Proceedings{Pauls2007a,
  Title                    = {Predictable and preventable missteps that are not "slips, trips and falls," but result in serious injuries},
  Year                     = {2007},

  Abstract                 = {Some types of missteps are not covered by the conventional label, "slips, trips and falls." This is not a trivial omission. Air steps, for example, are a major source of pedestrian injuries, which can be serious, even in cases where there is not an ensuing to the walking surface. One consequence of the latter is that such pedestrian problems get relatively little attention at the "research" end of the spectrum addressed in "Slips, Trips and Falls" conferences held to date and planned for August 2007. At the "practice" end there are also shortcomings in standards, codes and regulations as well as guides and other practice-oriented materials that could do much more to prevent or mitigate the problems. Examples, drawn from litigation-based consulting practice -- for example in relation to defective sidewalks and stairs, demonstrate how and why such other missteps occur and what can be done improve safety.},
