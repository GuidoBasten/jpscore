@Article{Koo2013,
  Title                    = {A comparative study of evacuation strategies for people with disabilities in high-rise building evacuation },
  Author                   = {Koo, Jeongin and Kim, Yong Seog and Kim, Byung-In and Christensen, Keith M.},
