@InProceedings{Hoogendoorn2004,
  Title                    = {Pedestrian flow modeling by adaptive control},
  Author                   = {Hoogendoorn, S. P.},
  Booktitle                = {TRB 2004 Annual Meeting},
  Year                     = {2004},

  Abstract                 = {Assessment tools for pedestrian walking speeds, comfort and travel times are important in for instance planning and geometric design of infrastructural facilities under regular and safety-critical circumstances (evacuations). This contribution focuses on pedestrian walking behavior theory and modeling. It is assumed that pedestrians are adaptive controllers that minimize the subjective predicted cost of walking, given changing walking objectives and pedestrian attitudes. The theory asserts that pedestrians predict the behavior of other pedestrians based on their observations and the assumed walking strategies of nearby pedestrians. The approach yields closed-form mathematical expressions predicting the acceleration and directional change of a pedestrian. It turns out that the model is able to predict the behavior of pedestrians accurately (lane formation, walking positions, faster-is-slower effect, etc.), as is shown by several application examples.},
