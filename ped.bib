@Article{Alonso-Marroquin2008,
  Title                    = {Bottlenecks in granular flow: When does an obstacle increase the flow rate in an hourglass?},
  Author                   = {Alonso-Marroquin, F. and Azeezullah, S.I. and Galindo-Torres, S. A. and Olsen-Kettle, L.M.},
  Year                     = {2008},

  Month                    = {dec},

  Abstract                 = {Bottlenecks occur in a wide range of applications from pedestrian and traffic flow to mineral
and food processing. We examine granular flow across a bottleneck and find that contrary to
expectations, the flow rate of particulate materials across a bottleneck actually increases if an
optimized obstacle is placed before it. The dependency of flow rate on obstacle diameter is derived
using a phenomenological velocity-density relationship, which peaks at a critical, finite density. This
relationship is in stark contrast to previous models of traffic flow where the velocity-density relation
is monotonically decreasing.},
  Comment                  = {granular materials, flow, bottlenecks, jamming, modelling,
microscopic},
