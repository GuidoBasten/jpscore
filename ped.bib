@Article{WangP2013,
  Title                    = {Conservation of energy for crowd self-driven motion},
  Author                   = {Wang, P. and Luh, Peter B. and Marsh, Kerry L.},
  Journal                  = {ArXiv e-prints},
  Year                     = {2013},

  Month                    = sep,

  Abstract                 = {For centuries, how an object moves given external forces is 
characterized by physical laws. As for creatures like human, 
their movements are selfÂ­driven, and the driving forces are 
generated by intention of people in a psychological sense. How 
to capture this characteristic in consistency with known physics 
laws is meaningful to understand human behaviors. 
By investigating a NewtonianÂ­like model of a pedestrian, this 
paper presents a fluidÂ­like model of crowd, showing that the 
physical concept of energy is also suited for psychological study

of human collective behaviors. An energy balance equation is 
derived to characterize how the energy is transformed between 
the psychological forms and physical forms: the psychological 
drive arises as a potential form of energy, which aims at either 
accelerating or compressing a crowd as transformed to energy 
in physical forms (e.g., kinetic energy or static energy). If such

transformation cannot be realized, disorder of the crowd may 
appear as a possible result. 
Based on our energyÂ­based analysis, a major gap is bridged 
among physical models, psychological findings and simulation 
results, and an insight is further presented to understand the 
paradoxical relationship of subjective wishes of human and 
objective result of their deeds in reality.},
  Adsnote                  = {Provided by the SAO/NASA Astrophysics Data System},
  Adsurl                   = {http://adsabs.harvard.edu/abs/2013arXiv1309.2785W},
  Archiveprefix            = {arXiv},
  Eprint                   = {1309.2785},
  File                     = {:pdf\\WangP2013.pdf:PDF},
