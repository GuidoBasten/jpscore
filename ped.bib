@InProceedings{Hoogendoorn2004a,
  Title                    = {Application of microscopic pedestrian flow simulation to station design evaluation in LisbonTrain Stations},
  Author                   = {Hoogendoorn, S. P. and Hauser, Miklos},
  Booktitle                = {TRB 2004 Annual Meeting},
  Year                     = {2004},

  Abstract                 = {This paper addresses assessment of station designs by application of pedestrian flow simulation models. In particular, station design issues pertain to the way access gates will effect the pedestrian flow operations, regarding levels-of-service, congestion levels, average walking times, delays incurred at the gates, etc. Furthermore, the effects of different system layouts are studied, as well as the difference between different gate systems (low and high capacity systems) in terms of processing passenger flows. Finally, we are interested in pedestrian flow operations in case of emergency situations. To answer these questions, pedestrian traffic operations for different station design alternatives are predicted using the dynamic microscopic pedestrian flow model NOMAD. This is done for reference situations (validation), as well as for the design alternatives. The NOMAD model has been extended with a new module describing pedestrian operations (i.e. user-optimal gate choice) at the gates. It turned out that the simple design guidelines used to set-up the design alternatives provided satisfactory level-of-service to the transferring pedestrians, and that the gates could be installed without compromising passenger safety. Nevertheless, the need to re-route, inefficient gate use and interactions between conflicting pedestrians flows caused small but significant delays to transferring pedestrians, that could not have been predicted without the use of an adequate micro simulation model.},
  File                     = {:pdf\\Hoogendoorn2004a.pdf:PDF},
  Keywords                 = {pedestrian flow modeling, micro simulation models, ex-ante studies, infrastructure design},
  Owner                    = {portz},
  Timestamp                = {2009.10.05}
}

